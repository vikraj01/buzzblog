  "description": "",
  "devDependencies": {
    "@parcel/resolver-default": "^2.12.0",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "shx": "^0.3.4",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@parcel/config-default": "^2.12.0",
    "@parcel/transformer-typescript-tsc": "^2.12.0",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-react": "^4.3.0",
    "dotenv": "^16.4.5",
    "firebase": "^10.12.2",
    "jwt-decode": "^4.0.0",
    "quill": "^2.0.2",
    "vite": "^5.2.13",
    "wayofjs": "^3.0.0"
  }
